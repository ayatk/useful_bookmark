{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "ordered-imports": true,
    "object-literal-sort-keys": false,
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "none"
      }
    ]
  }
}
